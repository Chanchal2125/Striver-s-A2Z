bool searchElement(vector<vector<int>> &MATRIX, int target) {
    // Write your code here.
    int r = MATRIX.size();
    int c = MATRIX[0].size();

    for(int i = 0; i < r; i++){
        for(int j = 0; j < c; j++){
            if(MATRIX[i][j] == target) return true;
        }
    }
    return false;
}
